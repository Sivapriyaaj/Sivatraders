           }
            var msgdata = {};  
            msgdata.name = $("#txtname").val();  
            msgdata.email = $("#txtemail").val();  
            msgdata.mobilenumber =  $("#txtphone").val();  
            msgdata.message = $("#txtmsg").val();
            $.ajax({
                cache: false,
                async: true,
                type: "POST",
                url: "@Url.Action("Register", "Home")",
                data: msgdata,
                success: function(data) {   
                    if(data=="Success"){ 
                      $('#spnmsg').css("color", "white");
                        $("#spnmsg").html("Message sent successfully.") 
                    }
                    else{
                         $("#spnmsg").html("Message not sent.") 
                    }
                  $("#txtname").val("")
                  $("#txtemail").val("")
                  $("#txtphone").val("")  
                   $("#txtmsg").val("")   
                }
            });
        });
      </script> 
   </body>
</html>